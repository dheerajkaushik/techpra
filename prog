
public class program1{
  public static void main(String[] args) {
    
    //Scanner scanner= new Scanner(System.in);

    
    int x=121;
int px=x;
    int reverse=0;
while (x>0) {
  

int digit=x%10;
reverse=reverse*10+digit;
x/=10;

}

if(reverse==px){System.out.println("palindrime");}
else System.out.println("no palindrome");
    




}}
